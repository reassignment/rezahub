local category = main:CreateCategory("Ball and Axe")

local section = category:CreateSection("Auto Win")

section:Create(
    "Button",
    "Click To Win",
    function()
        getyourname = game.Players.LocalPlayer.Character.Name 
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
        Game:GetService("Workspace").playerModels[getyourname].ball.CFrame = CFrame.new(-109.874924, 513.699951, 0.574999571, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        wait(0.1)
    end,
    {
        animated = true,
    }
)

