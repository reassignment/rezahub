section:Create(
    "KeyBind",
    "Hide/Unhide GUI", 
    function()
        if main.Motherframe.Visible == true then
	        main.Motherframe.Visible = false
	    elseif main.Motherframe.Visible == false then
	        main.Motherframe.Visible = true
	    end
    end,
    {
        default = Enum.KeyCode.M
    }
)
