local category = main:CreateCategory("Dunking Simulator")

local section = category:CreateSection("Auto Dunk")

section:Create(
    "Button",
    "Auto Dunk",
    function()
        while wait(2) do
		game:GetService("ReplicatedStorage").Remotes.DunkRequest:FireServer()
		end
    end,
    {
        animated = true,
    }
)

local category = main:CreateCategory("Credits")

local section = category:CreateSection("Credits")

section:Create(
    "TextBox",
    "Created By",
    function(input)
        
    end,
    {
        text = "Reassignment#3642"
    }
)

section:Create(
    "KeyBind",
    "Hide/Unhide GUI", 
    function()
        if main.Motherframe.Visible == true then
	        main.Motherframe.Visible = false
	    elseif main.Motherframe.Visible == false then
	        main.Motherframe.Visible = true
	    end
    end,
    {
        default = Enum.KeyCode.M
    }
)
