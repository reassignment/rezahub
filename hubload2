local category = main:CreateCategory("Ultra Clickers Simulator")

local section = category:CreateSection("AutoClick")

section:Create(
    "Button",
    "AutoClick",
    function()
        getfenv().Clicks = true -- or false
        while getfenv().Clicks do
        local Event = game:GetService("Workspace").Events.AddClick
        Event:FireServer()
        wait()
        end
    end,
    {
        animated = true,
    }
)

